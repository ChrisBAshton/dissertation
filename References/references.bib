@book{br:gameTheory,
Annote = {Online Dispute Resolution: Theory and Practice is a comprehensive sourcebook about ODR and the chapter `Artificial Intelligence and Online
Dispute Resolution' is particularly relevant to this project. An interesting problem raised from AI solutions, particularly those that use ``game theory as a basis for providing intelligent negotiation
support", is that the algorithms employed are usually only fair in terms of each disputant's desire being met, and not in terms of justice. An example provided by the paper is that in a Family Law dispute, if the parents were only interested in their own desires and not the interests of their children, game theories typically would not promote the interests of the children. The limitations of SmartResolution's maritime collision module and from any modules that might employ game theory as its core AI are hopefully overcome by the fact that each agent in the dispute is a lawyer, rather than the aggrieved disputants themselves.},
address = {The Hague, Netherlands},
editor = {Wahab, Mohamed S. Abdel and Katsh, Ethan and Rainey, Daniel},
publisher = {Eleven International Publishing},
title = {{Online Dispute Resolution: Theory and Practice}},
url = {http://www.ombuds.org/odrbook/Table_of_Contents.htm}
}

